{
    "compilerOptions": {
      "jsx": "react-jsx", // Ou "react" se estiver usando React <17
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "esnext",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "strict": true,
      "baseUrl": "."
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
  
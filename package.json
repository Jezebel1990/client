{
  "name": "boilerplate-apps-router",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "test": "jest",
    "test:watch": "jest --watch --maxWorkers=25%",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.cjs",
    "codegen": "apollo codegen:generate --target typescript --excludes=nodes_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "npm run schema:download && npm run schema:convert && npm run codegen",
    "test:ci": "jest --runInBand",
    "generate": "npx plop --plopfile generators/plopfile.js",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/runtime": "^7.26.0",
    "@netlify/plugin-nextjs": "^5.9.3",
    "@styled-icons/boxicons-solid": "^10.47.0",
    "@styled-icons/fa-brands": "^10.47.0",
    "date-fns": "^1.30.1",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "graceful-fs": "^4.2.11",
    "is-docker": "^3.0.0",
    "jsdom": "^25.0.1",
    "polished": "^4.3.1",
    "react": "^18",
    "react-dom": "^18",
    "react-slick": "^0.30.3",
    "styled-components": "^6.1.13",
    "styled-icons": "^10.47.1",
    "styled-media-query": "^2.1.2",
    "typescript": "^5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@chromatic-com/storybook": "^1.9.0",
    "@eslint/js": "^9.12.0",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-onboarding": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/nextjs": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "add": "^2.0.6",
    "apollo": "^2.34.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-styled-components": "^1.13.3",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-storybook": "^0.10.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-snapshot": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "lint-staged": "^15.2.10",
    "next": "^15.2.4",
    "plop": "^4.0.1",
    "prettier": "3.3.3",
    "storybook": "^8.3.6",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.8.1"
  },
  "overrides": {
    "graphql": "15.10.1"
  }
}
